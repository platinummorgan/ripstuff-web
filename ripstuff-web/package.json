{
  "name": "ripstuff-web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && next build",
    "postinstall": "prisma generate",
    "start": "next start",
    "lint": "next lint",
    "clean:uploads": "powershell -NoProfile -Command \"if (Test-Path ./public/uploads) { Remove-Item -Recurse -Force ./public/uploads; New-Item -ItemType Directory ./public/uploads | Out-Null }\"",
    "migrate:local-to-s3": "pnpm dlx tsx scripts/migrate-local-to-s3.ts",
    "prisma:generate": "pnpm exec prisma generate",
    "prisma:migrate": "pnpm exec prisma migrate dev",
    "prisma:migrate:init": "pnpm exec prisma migrate dev --name init",
    "prisma:studio": "pnpm exec prisma studio",
    "prisma:seed": "pnpm dlx tsx prisma/seed.ts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.894.0",
    "@aws-sdk/s3-request-presigner": "^3.894.0",
    "@ffmpeg-installer/ffmpeg": "^1.1.0",
    "@google/generative-ai": "^0.24.1",
    "@prisma/client": "^6.16.2",
    "@radix-ui/react-slot": "^1.2.3",
    "@upstash/ratelimit": "^2.0.6",
    "@upstash/redis": "^1.35.4",
    "@vercel/blob": "^2.0.0",
    "clsx": "^2.1.1",
    "dom-to-image": "^2.6.0",
    "dotenv": "^17.2.2",
    "fluent-ffmpeg": "^2.1.3",
    "html2canvas": "^1.4.1",
    "nanoid": "^5.1.6",
    "next": "14.2.32",
    "openai": "^5.22.0",
    "qrcode": "^1.5.4",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "resend": "^6.1.0",
    "sharp": "^0.34.4",
    "tailwind-merge": "^3.3.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/dom-to-image": "^2.6.7",
    "@types/node": "^20",
    "@types/qrcode": "^1.5.5",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.5.3",
    "prisma": "^6.16.2",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
